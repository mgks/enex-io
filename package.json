{
  "name": "enex-io",
  "version": "0.1.0",
  "description": "High-performance parser and generator for Evernote/Apple Notes (.enex) files.",
  "type": "module",
  "bin": {
    "enex-io": "dist/cli.js"
  },
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "sideEffects": false,
  "scripts": {
    "test": "node --import=tsx --test test/*.ts",
    "build": "tsup",
    "prepublishOnly": "npm run build && npm test",
    "lint": "xo"
  },
  "keywords": [
    "fs",
    "converter",
    "enex",
    "evernote",
    "apple-notes",
    "parser",
    "xml"
  ],
  "author": {
    "name": "mgks",
    "url": "https://mgks.dev"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mgks/enex-io.git"
  },
  "bugs": {
    "url": "https://github.com/mgks/enex-io/issues"
  },
  "homepage": "https://github.com/mgks/enex-io#readme",
  "funding": "https://github.com/sponsors/mgks",
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "commander": "^11.1.0",
    "dayjs": "^1.11.10"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "xo": "^0.56.0"
  },
  "xo": {
    "prettier": true,
    "rules": {
      "unicorn/filename-case": "off"
    }
  }
}